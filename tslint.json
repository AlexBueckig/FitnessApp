{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-plugin-prettier"],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ],
    "object-literal-sort-keys": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-any": true,
    "no-null-keyword": true,
    "no-empty-interface": false
  }
}
